{
	"name": "laravel/laravel",
	"type": "project",
	"description": "The Laravel Framework.",
	"keywords": [
        "framework",
        "laravel"
    ],
	"license": "MIT",
	"require": {
		"php": "^7.2.5",
		"danielme85/laravel-log-to-db": "^2.2",
		"fideloper/proxy": "^4.2",
		"fruitcake/laravel-cors": "^2.0",
		"guzzlehttp/guzzle": "^7.2",
		"inertiajs/inertia-laravel": "^0.4",
		"intervention/image": "^2.5",
		"laravel-notification-channels/telegram": "^0.5.1",
		"laravel-validation-rules/credit-card": "^1.3",
		"laravel/framework": "^7.0",
		"laravel/telescope": "3.5",
		"laravel/tinker": "^2.0",
		"nwidart/laravel-modules": "^7.1",
		"tightenco/ziggy": "^1.0",
		"webparking/laravel-queue-ensurer": "^1.3",
		"wewowweb/laravel-svelte-preset": "^0.1.8"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "^3.3",
		"barryvdh/laravel-ide-helper": "^2.7",
		"facade/ignition": "^2.0",
		"fzaninotto/faker": "^1.9.1",
		"mockery/mockery": "^1.3.1",
		"nunomaduro/collision": "^4.1",
		"phpunit/phpunit": "^8.5",
		"spatie/laravel-ray": "^1.13"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true,
    "platform": {
      "ext-pcntl": "7.3",
      "ext-posix": "7.3"
    }
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"autoload": {
		"files": [
            "app/helpers.php"
        ],
		"psr-4": {
			"App\\": "app/"
		},
		"classmap": [
            "database/seeds",
            "database/factories"
        ]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"compile": [
           "@php artisan config:clear",
           "@php artisan debugbar:clear",
           "@php artisan route:clear",
           "@php artisan cache:clear",
           "@php artisan view:clear",
           "@php artisan event:clear",
           "@php artisan key:generate --ansi",
           "@php artisan storage:link",
           "@php artisan migrate:fresh --force",
           "@php artisan module:seed",
           "@php artisan auth:clear-resets",
           "@php artisan route:cache",
           "@php artisan event:cache",
           "@php artisan config:cache",
           "@php artisan optimize",
           "composer dumpautoload"
        ],
		"recompile": [
           "@php artisan config:clear",
           "@php artisan debugbar:clear",
           "@php artisan route:clear",
           "@php artisan cache:clear",
           "@php artisan view:clear",
           "@php artisan event:clear",
           "@php artisan storage:link",
           "@php artisan route:cache",
           "@php artisan event:cache",
           "@php artisan config:cache",
           "@php artisan optimize",
           "composer dumpautoload"
        ],
		"reseed": [
            "@php artisan config:clear",
            "@php artisan debugbar:clear",
            "@php artisan route:clear",
            "@php artisan cache:clear",
            "@php artisan view:clear",
            "@php artisan event:clear",
            "@php artisan migrate:fresh",
            "@php artisan module:seed",
            "composer dumpautoload"
        ],
		"tf": "clear && vendor/bin/phpunit --filter",
		"ts": "vendor/bin/phpunit --testsuite",
		"all-tests": "@php artisan test && vendor/bin/phpunit app/Modules",
		"run-artisan-test": "@php artisan test",
     "post-update-cmd": [
            "@php artisan telescope:publish --ansi"
        ],
		"post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --write"
        ],
		"post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
		"post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
	}
}
